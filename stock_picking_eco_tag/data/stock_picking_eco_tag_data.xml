<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <function model="ir.model.data" name="_update_xmlids">
            <value model="base" eval="[{
                'xml_id': 'stock_picking_eco_tag.product_attribute_category',
                'record': obj().env['product.attribute'].search([('name', '=', 'CATEGORÍA')]) or obj().env['product.attribute'].create({'name': 'CATEGORÍA'}),
                'noupdate': True,
            }]"/>
        </function>
    </data>
</odoo>
