<odoo>
    <record id="view_stock_quant_tree_inventory_editable" model="ir.ui.view">
            <field name="name">stock.quant.tree.editable (in stock_inventory_move_custdate)</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.view_stock_quant_tree_inventory_editable" />
            <field name="arch" type="xml">
                <field name="quantity" position="after">
                    <field
                        name="quantity_at_date"
                        invisible="context.get('hide_quantity_at_date', True)"
                        string="At Date"
                    />
                </field>
            </field>
        </record>
</odoo>
